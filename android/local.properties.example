# Android SDK 및 Flutter SDK 경로 설정
# 이 파일을 복사하여 local.properties를 만들고 실제 경로로 교체하세요.
sdk.dir=YOUR_ANDROID_SDK_PATH
flutter.sdk=YOUR_FLUTTER_SDK_PATH
flutter.buildMode=debug
flutter.versionName=1.0.0
flutter.versionCode=1

# API Keys for Android (these values are used in build.gradle.kts)
# Google Maps API Key를 발급받아 설정하세요
MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY

# Kakao Native App Key를 발급받아 설정하세요
KAKAO_NATIVE_APP_KEY=YOUR_KAKAO_NATIVE_APP_KEY

